#home_video_fr.modal.fade
  .modal-dialog.modal-lg
    .modal-content
      .modal-header 
        button.close aria-hidden="true" data-dismiss="modal" type="button" &times;
        h2.modal-title How CakeFundraising works?
      .modal-body
        iframe id="youtube_video" title="How CakeFundraising works" width="100%" height="500px" src="//www.youtube.com/embed/_zEtobIg5W8" frameborder="0"
    /! /.modal-content
  /! /.modal-dialog
/! /.modal

#home_video_sp.modal.fade
  .modal-dialog.modal-lg
    .modal-content
      .modal-header 
        button.close aria-hidden="true" data-dismiss="modal" type="button" &times;
        h2.modal-title How CakeFundraising works?
      .modal-body
        iframe id="youtube_video" title="How CakeFundraising works" width="100%" height="500px" src="//www.youtube.com/embed/XXr73V1eS9M" frameborder="0"
    /! /.modal-content
  /! /.modal-dialog
/! /.modal

javascript:
  $('#home_video_fr').on('hidden.bs.modal', function (e) {
    $('#home_video_fr iframe').attr("src", jQuery("#home_video_fr iframe").attr("src"));
  });
  $('#home_video_sp').on('hidden.bs.modal', function (e) {
    $('#home_video_sp iframe').attr("src", jQuery("#home_video_sp iframe").attr("src"));
  });
